<?xml version='1.0' encoding='utf-8'?>
<widget id="com.phonegap.helloworld" version="1.1.0" xmlns="http://www.w3.org/ns/widgets" xmlns:gap="http://phonegap.com/ns/1.0">
    <name>MyA-Fib</name>
    <description>
        A PhoneGap app.
    </description>
    <author email="support@phonegap.com" href="http://phonegap.com">
        Mark K. Tuttle, MD
    </author>
    <icon src="icon180x180.png" />
    <content src="index.html" />
    <access origin="*" />
    <plugin name="cordova-plugin-whitelist" source="npm" />
    <plugin name="cordova-plugin-vibration" source="npm" spec="~2.1.6" />
    <access allows-arbitrary-loads-for-media="true" allows-arbitrary-loads-in-web-content="true" allows-local-networking="true" origin="medicineinnovation.com" />
    <engine name="browser" spec="~5.0.3" />
    <plugin name="cordova-plugin-urlhandler" spec="^0.7.2">
        <variable name="URL_SCHEME" value="myafib" />
    </plugin>
</widget>
